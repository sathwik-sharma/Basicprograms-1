// C Program to print all even numbers from 1 to 100

#include 
 
int main(){
    
    for(int i=1;i<=100;i++)
    {   
        // if number module i is equal to 0, then number is even
        if(i%2==0)
        {
            printf("%d\n", i);    
        }
    }
    return 0;
}

// C Program o print all odd numbers from 1 to 100

#include 
 
int main(){
    
    for(int i=1;i<=100;i++)
    {   
        // if number module i is equal to one, then number is odd
        if(i%2==1)
        {
            printf("%d\n", i);    
        }
    }
    return 0;
